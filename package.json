{
  "name": "@ledgerhq/metamask-keyring",
  "version": "0.3.3",
  "description": "A Metamask keyring implementation to bridge wallet operatations with Ledger Nano devices.",
  "repository": "git@github.com:LedgerHQ/metamask-keyring.git",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "format:check": "prettier --check \"src\"",
    "format:fix": "prettier --write \"src\"",
    "lint:check": "eslint --cache --ext .ts \"src\"",
    "lint:fix": "eslint --cache --fix --ext .ts \"src\"",
    "prepare": "yarn build && husky install",
    "build": "tsc -p tsconfig.json",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.7.0",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.ts": "yarn run lint:fix"
  },
  "dependencies": {
    "@ethereumjs/tx": "^4.2.0",
    "@ledgerhq/hw-app-eth": "6.34.7",
    "@ledgerhq/hw-transport": "6.28.8",
    "@metamask/eth-sig-util": "^7.0.0",
    "buffer": "^6.0.3",
    "ethereumjs-util": "^7.1.5"
  }
